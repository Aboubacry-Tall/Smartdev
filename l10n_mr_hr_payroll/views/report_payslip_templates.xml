<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <template id="report_payslip_mr" inherit_id="hr_payroll.report_payslip" primary="True">

    </template>

    <template id="report_payslip_mr_lang">
        <t t-call="web.html_container">
            <t t-foreach="docs" t-as="o">
                <t t-set="o" t-value="o.with_context(lang=o.employee_id.lang or o.env.lang)"/>
                <t t-call="l10n_mr_hr_payroll.report_payslip_mr" t-lang="o.env.lang"/>
            </t>
        </t>
    </template>
</odoo>
