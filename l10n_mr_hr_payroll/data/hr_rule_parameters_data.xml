        <field name="country_id" ref="base.mr"/>
    </record>
    <record id="l10n_mr_rule_parameter_its_value" model="hr.rule.parameter.value">
        <field name="parameter_value">[
            (0, 9000, 0.15, 0),
            (9000, 21000, 0.25, 900),
            (21000, float('inf'), 0.40, 4050),
        ]</field>
        <field name="rule_parameter_id" ref="l10n_mr_rule_parameter_its"/>
        <field name="date_from" eval="datetime(2017, 1, 1).date()"/>
    </record>

