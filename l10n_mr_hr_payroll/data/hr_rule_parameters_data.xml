    </record>
    <!-- CNSS pour l'Employé -->
    <record id="l10n_mr_rule_parameter_cnss_sal" model="hr.rule.parameter">
        <field name="name">CNSS Employé (%)</field>
        <field name="code">l10n_mr_cnss_employe</field>
