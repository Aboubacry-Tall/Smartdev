<?xml version="1.0" encoding="utf-8"?>

<odoo>
    <record id="l10n_mr_rule_parameter_seniority" model="hr.rule.parameter">
        <field name="name">Mauritania: Seniority</field>
        <field name="code">l10n_mr</field>
        <field name="country_id" ref="base.mr"/>
    </record>
    <record id="l10n_mr_rule_parameter_seniority_value" model="hr.rule.parameter.value">
        <field name="parameter_value">[
    (0, 2, 0.0),
    (2, 3, 0.02),
    (3, 4, 0.04),
    (4, 5, 0.06),
    (5, 6, 0.08),
    (6, 7, 0.10),
    (7, 8, 0.12),
    (8, 9, 0.14),
    (9, 10, 0.16),
    (10, 11, 0.18),
    (11, 12, 0.20),
    (12, 13, 0.22),
    (13, 14, 0.24),
    (14, 15, 0.26),
    (15, 16, 0.28),
    (16, 17, 0.29),
    (17, 100, 0.30),

]</field>
        <field name="rule_parameter_id" ref="l10n_mr_rule_parameter_seniority"/>
        <field name="date_from" eval="datetime(2017, 1, 1).date()"/>
    </record>




    <!-- CNSS configuration -->
    <record id="l10n_mr_rule_parameter_cnss" model="hr.rule.parameter">
